#include "fibo.hpp"
#include <CppUTest/CommandLineTestRunner.h>

TEST_GROUP (GroupFibo){ };
TEST(GroupFibo, test_fibo) {

  int result = fibo(0);
  CHECK_EQUAL(0, result);

  int result1 = fibo(1);
  CHECK_EQUAL(1, result1);

  int result2 = fibo(2);
  CHECK_EQUAL(1, result2);

  int result3 = fibo(3);
  CHECK_EQUAL(2, result3);

  int result4 = fibo(4);
  CHECK_EQUAL(3, result4);
  

}
